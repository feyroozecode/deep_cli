# DeepSeek CLI Chat Environment Variables
# Copy this file to .env and replace the values with your own

# Your DeepSeek API key (required)
DEEPSEEK_API_KEY=your_api_key_here

# Base URL for the DeepSeek API (optional)
DEEPSEEK_BASE_URL=https://inference.baseten.co/v1

# DeepSeek model to use (optional)
DEEPSEEK_MODEL=deepseek-ai/DeepSeek-R1-0528

# Default language (en, fr, ar)
DEEPSEEK_LANGUAGE=en